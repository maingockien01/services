{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "/",
  "compilerOptions": {
    "webpack": true,
    "deleteOutDir": true,
    "tsConfigPath": "api/ts.config.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "api",
      "entryFile": "main",
      "sourceRoot": "api",
      "compilerOptions": {
        "tsConfigPath": "api/ts.config.json"
      }

    }
  }
}
